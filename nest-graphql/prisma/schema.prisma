generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model point {
  point   Float? @db.Float
  user_id String @id @unique(map: "PK_user_id123") @db.VarChar(255)
  users   users  @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "point_ibfk_1")
}

model users {
  id       String  @id @db.VarChar(200)
  name     String? @db.VarChar(255)
  email    String? @db.VarChar(100)
  password String? @db.VarChar(255)
  point    point?
}
